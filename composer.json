{
    "name": "industrious-mouse/laravel-dotmailer",
    "description": "Laravel wrapper for the dotmailer-api-v2-client library (https://github.com/romanpitak/dotmailer-api-v2-client)",
    "authors": [
        {
            "name": "Christian Thomas",
            "email": "support@industriousmouse.co.uk"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "romanpitak/dotmailer-api-v2-client": "dev-patch-1"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/christian-thomas/dotMailer-API-v2-PHP-client"
        }
    ],
    "autoload": {
        "psr-0": {
            "IndustriousMouse\\LaravelDotmailer": "src/"
        }
    },
    "minimum-stability": "stable"
}
